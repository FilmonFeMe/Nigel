<h1>Welcome, <%= current_student.username.capitalize %></h1>

<div id="favoritedresources">
<h3>Favorited resources: </h3>
<% if current_student %>
  <ul>
    <% current_student.favorited_resources.each do |favorite| %>
      <li><h3><%= link_to favorite.title, favorite.link, target: '_blank' %></h3></li>
      <br>
    <% end %>
  </ul>
<% end %>
</div>